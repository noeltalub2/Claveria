<script src="/assets/static/js/components/dark.js"></script>
<script src="/assets/extensions/perfect-scrollbar/perfect-scrollbar.min.js"></script>

<script src="/assets/compiled/js/app.js"></script>

<script src="/vendor/sweetalert/sweetalert.js"></script>

<script> // Example of embedding EJS variables directly into JavaScript


<% if (error != "") { %>

    var errorMessages = "<%= error.map(errors => errors.msg).join(', ') %>";
        Swal.fire({
            position: 'top-end',
            icon: 'error',
            title: errorMessages,
            showConfirmButton: false,
            timer: 2000
        })
            <% } %>
    
<% if (error_msg != '') { %>
        Swal.fire({
            position: 'top-end',
            icon: 'error',
            title: '<%=error_msg%>',
            showConfirmButton: false,
            timer: 1500
        })
        <% } %>

    
 
<% if (success_msg != '') { %>
        Swal.fire({
            position: 'top-end',
            icon: 'success',
            title: '<%=success_msg%>',
            showConfirmButton: false,
            timer: 1500
        })
        <% } %>


</script>